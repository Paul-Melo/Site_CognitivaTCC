# Configurações do Flask
SECRET_KEY=your-secret-key-here
FLASK_ENV=development

# Configurações do Google Calendar
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REDIRECT_URI=http://localhost:5000/calendar/callback

# Configurações do Banco de Dados
DATABASE_URL=sqlite:///database/app.db

# Configurações de CORS
CORS_ORIGINS=http://localhost:5173,https://www.equilibriocognitivo.com.br

# Configurações de Email (opcional)
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your-email@gmail.com
SMTP_PASSWORD=your-app-password

# Configurações de ERP (opcional)
ERP_BASE_URL=https://your-erp-api.com
ERP_TOKEN=your-erp-token

